{
  "presets": [
    "@babel/preset-env",
    [
      "@babel/preset-env",
      { "targets": { "esmodules": true, "node": "current" } }
    ],
    ["@babel/preset-react", { "runtime": "automatic" }],
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "formatjs",
      {
        "idInterpolationPattern": "[sha512:contenthash:base64:6]",
        "ast": true
      }
    ]
  ]
}
